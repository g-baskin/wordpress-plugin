# WordPress App Test Environment Variables

# License System
LICENSE_API_URL=https://api.example.com/license
LICENSE_API_KEY=your_license_api_key_here

# Plugin Settings
PLUGIN_MODE=development
DEBUG_LOG=true

# REST API
API_BASE_PATH=/wp-json/wordpress-app-test/v1

# Database (optional, for custom tables if needed)
DB_HOST=localhost
DB_USER=wordpress
DB_PASSWORD=password
DB_NAME=wordpress

# Development
NODE_ENV=development
WP_ENV=local
